{
  "manifest_version": 2,

  "name": "Livejournal-IPs",
  "description": "Shows geo-ip information on Livejournal pages (country, city)",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["http://*.livejournal.com/*", "http://lentascope.net/*"],
      "js": ["jquery-2.1.3.min.js",  "content.js"]
    }
  ],

  "icons": { "16": "icon.png",
   "48": "icon48.png"
   }

}
